{
    "name": "nails\/module-tag",
    "keywords": [],
    "homepage": "https:\/\/nailsapp.co.uk\/modules\/tag",
    "description": "This module brings basic, global tagging to a project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "nails\/common": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "phpstan/phpstan": "0.*"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "analyse": "./vendor/bin/phpstan analyse -c .phpstan/config.neon"
    },
    "autoload": {
        "psr-4": {
            "Nails\\Tag\\": "src\/"
        }
    },
    "extra": {
        "nails": {
            "moduleName": "module-tag",
            "type": "module",
            "namespace": "Nails\\Tag\\",
            "data": {}
        }
    }
}
